{
  "name": "@adereso/desk-ui",
  "version": "1.0.2",
  "description": "UI elements for Adereso desk",
  "author": "Adereso Team FE",
  "license": "ISC",
  "homepage": "https://github.com/Adereso1/deskUI#readme",
  "main": "dist/desk-ui.ssr.js",
  "browser": "dist/desk-ui.esm.js",
  "module": "dist/desk-ui.esm.js",
  "unpkg": "dist/desk-ui.min.js",
  "repository": {
    "url": "git://github.com/Adereso1/deskUI.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist/*"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "v-tooltip": "^2.1.3",
    "vue-mention": "^1.0.0",
    "vue-tippy": "v4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "node-sass": "^6.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "sass-loader": "^10",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "engines": {
    "node": ">=12"
  }
}
